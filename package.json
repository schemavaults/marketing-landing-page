{
  "name": "@schemavaults/marketing-landing-page",
  "version": "0.0.19",
  "private": false,
  "license": "UNLICENSED",
  "description": "SchemaVaults landing page. Product features, waitlist, screenshots, testimonials, etc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/schemavaults/marketing-landing-page.git"
  },
  "dependencies": {
    "eslint": "9.17.0",
    "lucide-react": "0.508.0",
    "next-themes": "0.4.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "@schemavaults/theme": "0.22.7",
    "@schemavaults/ui": "0.11.101",
    "zod": "3.25.76",
    "@hookform/resolvers": "3.9.0"
  },
  "devDependencies": {
    "next": "15.3.3",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "bun-types": "1.2.17",
    "tsc-alias": "1.8.16",
    "typescript": "5.8.3",
    "postcss": "8.5.3",
    "tailwindcss": "3.4.17",
    "autoprefixer": "10.4.21"
  },
  "scripts": {
    "dev": "next dev --port 9817",
    "build:pkg": "tsc --project tsconfig.pkg.json && tsc-alias --project tsconfig.pkg.json",
    "build": "bun run build:pkg",
    "build:preview": "next build",
    "prebuild": "bun run clean",
    "clean": "rm -rf ./dist",
    "preview": "bun run dev"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*",
      "import": "./dist/*",
      "require": "./dist/*"
    },
    "./dist": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./dist/*": {
      "types": "./dist/*",
      "import": "./dist/*",
      "require": "./dist/*"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "bun@1.2.17"
}
